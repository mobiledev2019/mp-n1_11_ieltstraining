<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thithu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".listening.ListeningMain.ListeningActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".listening.ListeningQuest.ListeningQuestActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".listening.ListeningQuest.test.ListeningQuestActivitytest"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".calendar.TimePickerActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".listening.ListeningList.ListeningListActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <service android:name=".Service.MediaService"></service>
        <service android:name=".Service.MediaServiceAll"></service>
        <receiver android:name=".calendar.AlarmReceiver"></receiver>
    </application>

</manifest>